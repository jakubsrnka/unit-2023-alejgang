{
  "name": "unit-alejgang",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "lint:fix": "(prettier --plugin-search-dir . --write . || true) && eslint --fix .",
    "typegen": "pocketbase-typegen --json ./schema.json --out ./src/types/pocketbase.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/coverage-istanbul": "^0.27.1",
    "@vitest/ui": "^0.27.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fast-xml-parser": "^4.1.2",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prisma": "^4.12.0",
    "@prisma/client": "4.12.0",
    "sass": "^1.57.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "svelte-i18n": "^3.6.0",
    "svelte-markdown": "^0.2.3",
    "swiper": "^8.4.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module"
}